        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.moyan.mapper.SysUserMapper">

    <resultMap id="BaseResultMap" type="com.moyan.pojo.SysUser">
